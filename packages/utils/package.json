{
  "name": "@interchainjs/utils",
  "version": "1.21.0",
  "author": "Constructive <developers@constructive.io>",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./index.js",
      "types": "./index.d.ts"
    },
    "./*": {
      "import": "./esm/*.js",
      "require": "./*.js",
      "types": "./*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "homepage": "https://github.com/hyperweb-io/interchainjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperweb-io/interchainjs"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "test": "jest --config ./jest.config.js --verbose --bail",
    "build": "makage build",
    "build:dev": "makage build",
    "lint": "eslint . --fix",
    "prepack": "makage build"
  },
  "dependencies": {
    "@interchainjs/types": "workspace:*",
    "bech32": "^2.0.0",
    "decimal.js": "^10.4.3"
  },
  "devDependencies": {
    "@chain-registry/v2": "^1.71.186",
    "@chain-registry/v2-types": "^0.53.115",
    "makage": "^0.1.12"
  },
  "peerDependencies": {
    "@chain-registry/v2": "^1.71.186",
    "@chain-registry/v2-types": "^0.53.115"
  }
}
