{
  "name": "injective-vue",
  "version": "1.19.2",
  "description": "injective-vue is a JavaScript library providing vue hooks for interacting with injective sdk.",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./index.js",
      "types": "./index.d.ts"
    },
    "./*": {
      "import": "./esm/*.js",
      "require": "./*.js",
      "types": "./*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "author": "Constructive <developers@constructive.io>",
  "homepage": "https://github.com/hyperweb-io/interchainjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperweb-io/interchainjs"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "copy": "copyfiles -f ../../LICENSE-MIT ../../LICENSE-Apache README.md package.json dist",
    "clean": "rimraf dist/**",
    "build": "npm run clean; tsc; tsc -p tsconfig.esm.json; npm run fix-esm; npm run copy",
    "build:dev": "npm run clean; tsc --declarationMap; tsc -p tsconfig.esm.json; npm run copy",
    "lint": "eslint . --fix",
    "prepare": "npm run build",
    "fix-esm": "tsc-esm-fix --target=dist/esm"
  },
  "dependencies": {
    "@interchainjs/cosmos": "^1.19.2",
    "@interchainjs/cosmos-types": "^1.19.1",
    "@interchainjs/encoding": "^1.18.1",
    "@interchainjs/math": "^1.18.1",
    "@interchainjs/pubkey": "^1.19.1",
    "@interchainjs/types": "^1.19.1",
    "@interchainjs/utils": "^1.19.1",
    "@noble/hashes": "^1.3.1",
    "decimal.js": "^10.4.3"
  },
  "devDependencies": {
    "@tanstack/vue-query": "^5.62.7",
    "tsc-esm-fix": "^3.1.2",
    "vue": "^3.5.13"
  },
  "peerDependencies": {
    "@tanstack/vue-query": "^5.62.7",
    "vue": "^3.5.13"
  },
  "keywords": [
    "cosmos",
    "blockchain",
    "transaction",
    "cosmjs",
    "wallet"
  ],
  "gitHead": "74dec3776f908660a4450aac3cf948b875f14562"
}
